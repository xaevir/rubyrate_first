
function blah (ev) { ev(); };

function tostr (x) {


    var ret = "";

    switch (x) {
    case 0:
	twait { blah (mkevent ()); }
	ret = "ZED";
	break;
    case 17:
    case 1:
	twait { blah (mkevent ()); }
	ret = "ONE";
	break;
    case 2:
	twait { blah (mkevent ()); }
	console.log ("fall through!");
    case 18:
    case 3:
	twait { blah (mkevent ()); }
	console.log ("and again!");
    default:
	twait { blah (mkevent ()); }
	ret = "don't know";
	break;
    }

    console.log (ret);
}

tostr (0);
tostr (1);
tostr (2);
tostr (3);
tostr (4);
tostr (18);
tostr (17);
